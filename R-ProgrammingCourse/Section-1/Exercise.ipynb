   "source": [
    "N <- 10\n",
    "counter <- 0\n",
    "for (i in rnorm(N)) {\n",
    "    if(i < 1 & i > -1)\n",
    "    {\n",
    "        counter <- counter + 1\n",
    "    }\n",
    "}\n",
    "res <- counter / N\n",
    "print(res)"
   ]
