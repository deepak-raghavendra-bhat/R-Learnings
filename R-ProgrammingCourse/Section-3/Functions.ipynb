       "     min(..., na.rm = FALSE)\n",
       "     \n",
       "     pmax(..., na.rm = FALSE)\n",
       "     pmin(..., na.rm = FALSE)\n",
       "     \n",
       "     pmax.int(..., na.rm = FALSE)\n",
       "     pmin.int(..., na.rm = FALSE)\n",
       "     \n",
